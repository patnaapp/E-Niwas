<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:background="@color/holo_gray_bright"
    android:layout_height="match_parent">

    <LinearLayout

        android:id="@+id/ll_header"
        android:layout_width="match_parent"
        android:background="@color/holo_gray_light"
        android:layout_height="wrap_content"
        android:gravity="center_horizontal"
        android:orientation="vertical">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:text="E-NIWAS"
            android:textColor="@color/colorPrimary"
            android:textSize="22sp"
            android:textStyle="bold" />

        <ImageView
            android:visibility="gone"
            android:layout_marginTop="12dp"
            android:id="@+id/iv_logo"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/logo" />

        <TextView
            android:visibility="gone"
            android:textColor="#DD2424"
            android:layout_width="match_parent"
            android:gravity="center"
            android:textStyle="bold"
            android:textSize="21sp"
            android:text="e-Labharthi"

            android:layout_height="wrap_content"/>
        <TextView
            android:textColor="#DD2424"
            android:layout_width="match_parent"
            android:gravity="center"
            android:textStyle="bold"
            android:textSize="15sp"
            android:text="BUILDING CONSTRUCTION DEPARTMENT,BIHAR"
            android:layout_height="wrap_content"/>


    </LinearLayout>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="New Entry"
        android:layout_below="@id/ll_header"

        android:id="@+id/tv_desc"
        android:layout_margin="8dp"
        android:textColor="@color/holo_green_dark"
        android:textAlignment="center"/>
    <ScrollView

        android:id="@+id/reg_form"
        android:layout_below="@+id/tv_desc"
        android:layout_above="@+id/btn_proceed"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginBottom="2dp"
        android:layout_marginLeft="2dp"
        android:layout_marginRight="2dp"
        android:layout_marginTop="2dp">
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <ImageView
                android:layout_margin="20dp"
                android:visibility="gone"
                android:id="@+id/iv_logo1"
                android:layout_width="90dp"
                android:layout_height="90dp"
                android:src="@drawable/logo"
                android:padding="4dp"
                android:background="@drawable/circular_shape_white"
                android:layout_centerHorizontal="true"/>

            <LinearLayout
                android:layout_width="match_parent"
                android:id="@+id/ll_all"

                android:layout_height="wrap_content"
                android:orientation="vertical">


                <LinearLayout
                    android:layout_marginBottom="12dp"
                    android:layout_width="fill_parent"

                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:padding="10dp"
                    android:orientation="vertical">
                    <LinearLayout

                        android:layout_width="fill_parent"

                        android:layout_height="wrap_content"


                        android:weightSum="2"
                        android:orientation="horizontal">
                        <LinearLayout

                            android:layout_marginBottom="12dp"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:orientation="vertical">

                            <TextView
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:text="Building Division : [*]"
                                android:textColor="@color/color_black"
                                android:textSize="12sp"/>

                            <Spinner
                                android:background="@drawable/spinner_bg1"
                                android:layout_marginTop="4dp"
                                android:id="@+id/sp_building_div"
                                android:layout_width="match_parent"
                                android:layout_height="36dp" />
                        </LinearLayout>


                    </LinearLayout>


                    <LinearLayout

                        android:layout_width="fill_parent"

                        android:layout_height="wrap_content"

                        android:orientation="vertical">
                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"

                            android:text="Property Type : [*]"
                            android:textStyle="bold" />
                        <LinearLayout
                            android:id="@+id/chk_property_type"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">



                            <CheckBox
                                android:id="@+id/chk_open_land"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="Open Land" />

                            <CheckBox
                                android:id="@+id/chk_existing_building"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="Land with existing building" />



                        </LinearLayout>
                    </LinearLayout>

                    <LinearLayout

                        android:layout_width="fill_parent"

                        android:layout_height="wrap_content"

                        android:orientation="vertical">
                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"

                            android:text="Area: [*]"
                            android:textStyle="bold" />
                        <LinearLayout
                            android:id="@+id/chk_area_type"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">



                            <CheckBox
                                android:id="@+id/chk_urban"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="Urban" />

                            <CheckBox
                                android:id="@+id/chk_rural"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="Rural" />



                        </LinearLayout>
                    </LinearLayout>
                    <LinearLayout

                        android:layout_width="fill_parent"

                        android:layout_height="wrap_content"
                        android:layout_marginTop="4dp"

                        android:weightSum="2"
                        android:orientation="horizontal">
                        <LinearLayout

                            android:layout_marginBottom="12dp"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:orientation="vertical">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="District : [*]"
                                android:textColor="@color/color_black"
                                android:textSize="12sp"/>

                            <Spinner
                                android:background="@drawable/spinner_bg1"
                                android:layout_marginTop="4dp"
                                android:id="@+id/sp_dist"
                                android:layout_width="match_parent"
                                android:layout_height="36dp" />
                        </LinearLayout>
                        <View
                            android:layout_width="4dp"
                            android:layout_height="match_parent"/>
                        <LinearLayout

                            android:layout_marginBottom="12dp"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:orientation="vertical">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="Sub-Divisison : [*]"
                                android:textColor="@color/color_black"
                                android:textSize="12sp"/>

                            <Spinner
                                android:background="@drawable/spinner_bg1"
                                android:layout_marginTop="4dp"
                                android:id="@+id/sp_subdiv"
                                android:layout_width="match_parent"
                                android:layout_height="36dp" />
                        </LinearLayout>
                    </LinearLayout>

                    <LinearLayout

                        android:layout_width="fill_parent"

                        android:layout_height="wrap_content"


                        android:weightSum="2"
                        android:orientation="horizontal">
                        <LinearLayout
                            android:layout_marginBottom="12dp"
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:gravity="center"
                            android:orientation="vertical">

                            <LinearLayout
                                android:layout_width="fill_parent"
                                android:layout_height="wrap_content">

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="Block: [*]"
                                    android:textAppearance="?android:attr/textAppearanceSmall"
                                    android:textSize="13sp"
                                    android:textStyle="normal"
                                    android:textColor="@color/color_black"/>

                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="fill_parent"
                                android:layout_height="wrap_content"
                                android:orientation="vertical">

                                <Spinner
                                    android:id="@+id/sp_block"
                                    android:layout_width="match_parent"
                                    android:layout_height="36dp"
                                    android:layout_marginTop="4dp"
                                    android:background="@drawable/spinner_bg1" />

                            </LinearLayout>


                        </LinearLayout>
                        <View
                            android:layout_width="4dp"
                            android:layout_height="match_parent"/>
                        <LinearLayout
                            android:layout_marginBottom="12dp"
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:gravity="center"
                            android:layout_weight="1"
                            android:orientation="vertical">

                            <LinearLayout
                                android:layout_width="fill_parent"
                                android:layout_height="wrap_content">

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="Ward/Panchayat :"
                                    android:textAppearance="?android:attr/textAppearanceSmall"
                                    android:textSize="13sp"
                                    android:textStyle="normal"
                                    android:textColor="@color/color_black"/>

                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="fill_parent"
                                android:layout_height="wrap_content"
                                android:orientation="vertical">

                                <Spinner
                                    android:id="@+id/sp_ward_pan"
                                    android:layout_width="match_parent"
                                    android:layout_height="36dp"
                                    android:layout_marginTop="4dp"
                                    android:background="@drawable/spinner_bg1" />

                            </LinearLayout>

                        </LinearLayout>
                    </LinearLayout>
                    <LinearLayout
                        android:layout_marginBottom="12dp"
                        android:layout_width="fill_parent"
                        android:layout_height="wrap_content"
                        android:gravity="center"
                        android:orientation="vertical">
                        <LinearLayout

                            android:layout_width="fill_parent"

                            android:layout_height="wrap_content"
                            android:layout_marginTop="4dp"

                            android:weightSum="2"
                            android:orientation="horizontal">
                            <LinearLayout
                                android:layout_marginTop="5dp"
                                android:layout_marginBottom="12dp"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:orientation="vertical">

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="Pincode : [*]"
                                    android:textColor="@color/color_black"
                                    android:textSize="12sp"/>

                                <EditText
                                    android:layout_marginTop="4dp"
                                    android:id="@+id/edt_pincode"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:background="@drawable/textboxshape"
                                    android:hint="Pincode"
                                    android:padding="10dp"
                                    android:inputType="textCapWords"
                                    android:maxLength="20"
                                    android:singleLine="true"
                                    android:textColor="#222222"
                                    android:focusableInTouchMode="true"
                                    android:textSize="15sp" />
                            </LinearLayout>
                            <View
                                android:layout_width="4dp"
                                android:layout_height="match_parent"/>
                            <LinearLayout
                                android:layout_marginTop="5dp"
                                android:layout_marginBottom="12dp"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:orientation="vertical">

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="Thana No :"
                                    android:textColor="@color/color_black"
                                    android:textSize="12sp"/>

                                <EditText
                                    android:layout_marginTop="4dp"
                                    android:id="@+id/edt_thana"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:background="@drawable/textboxshape"
                                    android:hint="Thana No"
                                    android:padding="10dp"
                                    android:inputType="textCapWords"
                                    android:maxLength="20"
                                    android:singleLine="true"
                                    android:textColor="#222222"
                                    android:focusableInTouchMode="true"
                                    android:textSize="15sp" />
                            </LinearLayout>
                        </LinearLayout>

                        <LinearLayout

                            android:layout_width="fill_parent"

                            android:layout_height="wrap_content"
                            android:layout_marginTop="4dp"

                            android:weightSum="2"
                            android:orientation="horizontal">
                            <LinearLayout
                                android:layout_marginTop="5dp"
                                android:layout_marginBottom="12dp"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:orientation="vertical">

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="Khata :"
                                    android:textColor="@color/color_black"
                                    android:textSize="12sp"/>

                                <EditText
                                    android:layout_marginTop="4dp"
                                    android:id="@+id/edt_khata"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:background="@drawable/textboxshape"
                                    android:hint="Khata"
                                    android:padding="10dp"
                                    android:inputType="textCapWords"
                                    android:maxLength="20"
                                    android:singleLine="true"
                                    android:textColor="#222222"
                                    android:focusableInTouchMode="true"
                                    android:textSize="15sp" />
                            </LinearLayout>
                            <View
                                android:layout_width="4dp"
                                android:layout_height="match_parent"/>
                            <LinearLayout
                                android:layout_marginTop="5dp"
                                android:layout_marginBottom="12dp"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:orientation="vertical">

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="Khesra No :"
                                    android:textColor="@color/color_black"
                                    android:textSize="12sp"/>

                                <EditText
                                    android:layout_marginTop="4dp"
                                    android:id="@+id/edt_khesra"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:background="@drawable/textboxshape"
                                    android:hint="Khesra No"
                                    android:padding="10dp"
                                    android:inputType="textCapWords"
                                    android:maxLength="20"
                                    android:singleLine="true"
                                    android:textColor="#222222"
                                    android:focusableInTouchMode="true"
                                    android:textSize="15sp" />
                            </LinearLayout>
                        </LinearLayout>

                        <LinearLayout

                            android:layout_width="fill_parent"

                            android:layout_height="wrap_content"
                            android:layout_marginTop="4dp"

                            android:weightSum="2"
                            android:orientation="horizontal">
                            <LinearLayout
                                android:layout_marginTop="5dp"
                                android:layout_marginBottom="12dp"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:orientation="vertical">

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="Chauhaddi North :"
                                    android:textColor="@color/color_black"
                                    android:textSize="12sp"/>

                                <EditText
                                    android:layout_marginTop="4dp"
                                    android:id="@+id/edt_nrth_chauhaddi"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:background="@drawable/textboxshape"
                                    android:hint="Chauhaddi North"
                                    android:padding="10dp"
                                    android:inputType="textCapWords"
                                    android:maxLength="20"
                                    android:singleLine="true"
                                    android:textColor="#222222"
                                    android:focusableInTouchMode="true"
                                    android:textSize="15sp" />
                            </LinearLayout>
                            <View
                                android:layout_width="4dp"
                                android:layout_height="match_parent"/>
                            <LinearLayout
                                android:layout_marginTop="5dp"
                                android:layout_marginBottom="12dp"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:orientation="vertical">

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="Chauhaddi South :"
                                    android:textColor="@color/color_black"
                                    android:textSize="12sp"/>

                                <EditText
                                    android:layout_marginTop="4dp"
                                    android:id="@+id/edt_south_chauhaddi"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:background="@drawable/textboxshape"
                                    android:hint="Chauhaddi South "
                                    android:padding="10dp"
                                    android:inputType="textCapWords"
                                    android:maxLength="20"
                                    android:singleLine="true"
                                    android:textColor="#222222"
                                    android:focusableInTouchMode="true"
                                    android:textSize="15sp" />
                            </LinearLayout>
                        </LinearLayout>

                        <LinearLayout

                            android:layout_width="fill_parent"

                            android:layout_height="wrap_content"
                            android:layout_marginTop="4dp"

                            android:weightSum="2"
                            android:orientation="horizontal">
                            <LinearLayout
                                android:layout_marginTop="5dp"
                                android:layout_marginBottom="12dp"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:orientation="vertical">

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="Chauhaddi East :"
                                    android:textColor="@color/color_black"
                                    android:textSize="12sp"/>

                                <EditText
                                    android:layout_marginTop="4dp"
                                    android:id="@+id/edt_neast_chauhaddi"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:background="@drawable/textboxshape"
                                    android:hint="Chauhaddi East"
                                    android:padding="10dp"
                                    android:inputType="textCapWords"
                                    android:maxLength="20"
                                    android:singleLine="true"
                                    android:textColor="#222222"
                                    android:focusableInTouchMode="true"
                                    android:textSize="15sp" />
                            </LinearLayout>
                            <View
                                android:layout_width="4dp"
                                android:layout_height="match_parent"/>
                            <LinearLayout
                                android:layout_marginTop="5dp"
                                android:layout_marginBottom="12dp"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:orientation="vertical">

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="Chauhaddi West :"
                                    android:textColor="@color/color_black"
                                    android:textSize="12sp"/>

                                <EditText
                                    android:layout_marginTop="4dp"
                                    android:id="@+id/edt_west_chauhaddi"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:background="@drawable/textboxshape"
                                    android:hint="Chauhaddi West"
                                    android:padding="10dp"
                                    android:inputType="textCapWords"
                                    android:maxLength="20"
                                    android:singleLine="true"
                                    android:textColor="#222222"
                                    android:focusableInTouchMode="true"
                                    android:textSize="15sp" />
                            </LinearLayout>
                        </LinearLayout>


                        <LinearLayout

                            android:layout_width="fill_parent"

                            android:layout_height="wrap_content"
                            android:layout_marginTop="4dp"

                            android:weightSum="2"
                            android:orientation="horizontal">
                            <LinearLayout
                                android:layout_marginTop="5dp"
                                android:layout_marginBottom="12dp"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:orientation="vertical">

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="Area of land in sq feet : [*]"
                                    android:textColor="@color/color_black"
                                    android:textSize="12sp"/>

                                <EditText
                                    android:layout_marginTop="4dp"
                                    android:id="@+id/edt_land_area"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:background="@drawable/textboxshape"
                                    android:hint="Area of land in sq feet"
                                    android:padding="10dp"
                                    android:inputType="textCapWords"
                                    android:maxLength="20"
                                    android:singleLine="true"
                                    android:textColor="#222222"
                                    android:focusableInTouchMode="true"
                                    android:textSize="15sp" />
                            </LinearLayout>
                            <View
                                android:layout_width="4dp"
                                android:layout_height="match_parent"/>
                            <LinearLayout
                                android:layout_marginTop="5dp"
                                android:layout_marginBottom="12dp"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:orientation="vertical">

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="No of big trees on th land :"
                                    android:textColor="@color/color_black"
                                    android:textSize="12sp"/>

                                <EditText
                                    android:layout_marginTop="4dp"
                                    android:id="@+id/edt_trees_no"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:background="@drawable/textboxshape"
                                    android:hint="No of Trees"
                                    android:padding="10dp"
                                    android:inputType="textCapWords"
                                    android:maxLength="20"
                                    android:singleLine="true"
                                    android:textColor="#222222"
                                    android:focusableInTouchMode="true"
                                    android:textSize="15sp" />
                            </LinearLayout>
                        </LinearLayout>
                        <LinearLayout
                            android:layout_marginTop="5dp"
                            android:layout_marginBottom="12dp"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="Details of trees:"
                                android:textColor="@color/color_black"
                                android:textSize="12sp"/>

                            <EditText
                                android:layout_marginTop="4dp"
                                android:id="@+id/et_trees_details"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:background="@drawable/textboxshape"
                                android:hint="Details of trees "
                                android:padding="10dp"
                                android:inputType="textCapWords"
                                android:maxLength="50"
                                android:singleLine="true"
                                android:textColor="#222222"
                                android:focusableInTouchMode="true"
                                android:textSize="15sp" />
                        </LinearLayout>


                        <LinearLayout

                            android:layout_width="fill_parent"

                            android:layout_height="wrap_content"


                            android:weightSum="2"
                            android:orientation="horizontal">
                            <LinearLayout

                                android:layout_marginBottom="12dp"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:layout_weight="1"
                                android:orientation="vertical">

                                <TextView
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:text="Is there any building : [*]"
                                    android:textColor="@color/color_black"
                                    android:textSize="12sp"/>

                                <Spinner
                                    android:background="@drawable/spinner_bg1"
                                    android:layout_marginTop="4dp"
                                    android:id="@+id/sp_bulding_check"
                                    android:layout_width="match_parent"
                                    android:layout_height="36dp" />
                            </LinearLayout>

                        </LinearLayout>

                        <LinearLayout
                            android:layout_marginTop="5dp"
                            android:id="@+id/ll_admindept"
                            android:visibility="gone"
                            android:layout_marginBottom="12dp"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_weight="1"
                            android:orientation="vertical">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="Name of administrative department :"
                                android:textColor="@color/color_black"
                                android:textSize="12sp"/>

                            <EditText
                                android:layout_marginTop="4dp"
                                android:id="@+id/edt_admin_dept"
                                android:layout_width="match_parent"
                                android:layout_height="wrap_content"
                                android:background="@drawable/textboxshape"
                                android:hint="Name of administrative department"
                                android:padding="10dp"
                                android:inputType="textCapWords"
                                android:maxLength="20"
                                android:singleLine="true"
                                android:textColor="#222222"
                                android:focusableInTouchMode="true"
                                android:textSize="15sp" />
                        </LinearLayout>
                    </LinearLayout>




                </LinearLayout>
            </LinearLayout>
        </RelativeLayout>
    </ScrollView>
    <Button
        android:layout_gravity="center"
        android:layout_centerInParent="true"
        android:id="@+id/btn_proceed"
        android:layout_above="@+id/footer"
        android:layout_width="150dp"
        android:layout_height="36dp"
        android:layout_marginTop="20dp"
        android:layout_marginBottom="10dp"
        android:background="@drawable/buttonshape"
        android:text="    Proceed  "
        android:textColor="#ffff"
        android:textSize="17sp" />
    <include
        android:id="@+id/footer"
        layout="@layout/footer"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true" />

</RelativeLayout>